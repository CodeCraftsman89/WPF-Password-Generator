<Window x:Name="PassGenWndw"  x:Class="PassGen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PassGen"
        mc:Ignorable="d"
        Title="Password Generator" Height="206" Width="301" Background="White" Icon="/key_262538.ico">
    <Grid>
        <TextBox x:Name="TxtPass" HorizontalAlignment="Left" Margin="0,10,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="291" TextChanged="TxtPass_TextChanged" Height="40" FontSize="25" BorderBrush="#FF0040FF"/>
        <Button x:Name="GenPassBtn" Content="Generate" HorizontalAlignment="Left" Margin="0,55,0,0" VerticalAlignment="Top" Click="GenPassBtn_Click" Height="119" Width="291" Background="#FF9FFF9A" BorderBrush="#FF295831" FontSize="30"/>
    </Grid>
</Window>
